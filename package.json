{
  "name": "voorhoede-dragonfly",
  "version": "1.0.0",
  "description": "Website for De Voorhoede",
  "author": "De Voorhoede (https://www.voorhoede.nl)",
  "homepage": "https://github.com/voorhoede/voorhoede-dragonfly#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/voorhoede/voorhoede-dragonfly"
  },
  "engines": {
    "node": ">=10.8.0",
    "npm": ">=6.2.0"
  },
  "private": true,
  "scripts": {
    "analyze": "nuxt build --analyze",
    "prebuild": "run-s prebuild:clean prebuild:predump prebuild:dump",
    "prebuild:clean": "rimraf src/client/static/data src/client/static/_redirects",
    "prebuild:predump": "dato dump",
    "prebuild:dump": "npm run data",
    "build": "NODE_ENV=production run-s build:*",
    "build:client": "nuxt generate",
    "postbuild": "node _scripts/build-feed/index.js",
    "predev": "run-s prebuild:clean prebuild:predump",
    "dev": "PORT=3278 nuxt",
    "data": "node _scripts/data-dump/index.js",
    "lint": "run-s lint:*",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:css": "stylelint '**/*.{vue,css}'",
    "lintfix": "run-s lintfix:*",
    "lintfix:js": "npm run lint:js -- --fix",
    "lintfix:css": "npm run lint:css -- --fix",
    "precommit": "npm run lint",
    "proxy": "ngrok http 3278",
    "start": "nuxt start",
    "test": "ava -v",
    "test:watch": "ava -vw"
  },
  "dependencies": {
    "@nuxtjs/google-analytics": "^2.0.2",
    "babel-polyfill": "^6.26.0",
    "dayjs": "^1.8.14",
    "dotenv-safe": "6.1.0",
    "nuxt-i18n": "^4.1.0",
    "nuxt": "1.4.4",
    "prismjs": "^1.15.0",
    "unfetch": "^4.0.1",
    "url-search-params-polyfill": "^5.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.3.1",
    "ava": "^1.2.0",
    "babel-eslint": "8.2.1",
    "backoff": "^2.5.0",
    "chalk": "^2.4.1",
    "cheerio": "^1.0.0-rc.2",
    "datocms-client": "0.6.2",
    "eslint": "4.15.0",
    "eslint-friendly-formatter": "3.0.0",
    "eslint-loader": "1.7.1",
    "eslint-plugin-vue": "5.0.0",
    "glob": "7.1.3",
    "husky": "0.14.3",
    "iso-639-1": "^2.0.3",
    "lodash": "^4.17.11",
    "ngrok": "3.1.0",
    "node-fetch": "^2.2.0",
    "npm-run-all": "4.1.5",
    "postcss-calc": "^7.0.1",
    "postcss-custom-properties": "^8.0.9",
    "postcss-import": "^12.0.1",
    "promise-polyfill": "^8.1.0",
    "proxyquire": "^2.1.0",
    "raw-loader": "^0.5.1",
    "rimraf": "2.6.2",
    "sinon": "^7.2.3",
    "stylelint": "9.9.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-processor-html": "1.0.0",
    "util": "^0.11.0",
    "vue-svg-loader": "0.5.0",
    "vue-template-compiler": "^2.5.17",
    "xmlbuilder": "^11.0.0"
  },
  "browserslist": [
    "ie 11",
    "last 2 versions"
  ],
  "ava": {
    "babel": false,
    "compileEnhancements": false
  }
}
