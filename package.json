{
  "name": "voorhoede-dragonfly",
  "version": "1.0.0",
  "description": "Website for De Voorhoede",
  "author": "De Voorhoede (https://www.voorhoede.nl)",
  "homepage": "https://github.com/voorhoede/voorhoede-dragonfly#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/voorhoede/voorhoede-dragonfly"
  },
  "private": true,
  "scripts": {
    "analyze": "nuxt build --analyze",
    "prebuild": "npm run data",
    "build": "NODE_ENV=production run-s build:*",
    "build:client": "nuxt generate",
    "build:guide": "build-storybook -c config/storybook -o dist/client/guide/",
    "predata": "rimraf src/client/static/data",
    "data": "dato dump --config=./config/dato/dato.config.js",
    "dev": "PORT=3278 nuxt",
    "guide": "start-storybook -p 3279 -c config/storybook -s ./src/client/static",
    "lint": "run-s lint:*",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:css": "stylelint '**/*.{vue,css}'",
    "lintfix": "run-s lintfix:*",
    "lintfix:js": "npm run lint:js -- --fix",
    "lintfix:css": "npm run lint:css -- --fix",
    "new-component": "node _scripts/new-component/index.js --component",
    "precommit": "npm run lint",
    "proxy": "ngrok http 3278",
    "start": "nuxt start"
  },
  "dependencies": {
    "dotenv-safe": "6.0.0",
    "nuxt": "1.0.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.4.8",
    "@storybook/addon-knobs": "3.4.8",
    "@storybook/addon-options": "3.4.8",
    "@storybook/vue": "3.4.8",
    "babel-eslint": "8.2.1",
    "commander": "2.16.0",
    "datocms-client": "0.6.2",
    "eslint": "4.15.0",
    "eslint-friendly-formatter": "3.0.0",
    "eslint-loader": "1.7.1",
    "eslint-plugin-vue": "4.0.0",
    "glob": "7.1.2",
    "husky": "0.14.3",
    "lodash": "4.17.10",
    "ngrok": "3.0.1",
    "npm-run-all": "4.1.3",
    "nuxt-i18n": "4.1.0",
    "postcss-custom-properties": "7.0.0",
    "rimraf": "2.6.2",
    "storybook-readme": "3.3.0",
    "stylelint": "9.3.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-processor-html": "1.0.0",
    "vue-svg-loader": "0.5.0"
  },
  "browserslist": [
    "ie 11",
    "last 2 versions"
  ]
}
